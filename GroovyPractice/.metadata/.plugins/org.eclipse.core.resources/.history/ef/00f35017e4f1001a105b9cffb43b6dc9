package com.test.demo

class Fruits {
	
	private String fruitName
	private String fruitColor
	
	//setters
	def setFruitName(String name) {
		fruitName = name
	}
	
	def setFruitColor(String color) {
		fruitColor = color
	}
	
	//getters
	def getFruitName() {
		return fruitName
	}
	
	def getFruitColor() {
		return fruitColor
	}
	
	
	static void main(args) {
		
	}
}
